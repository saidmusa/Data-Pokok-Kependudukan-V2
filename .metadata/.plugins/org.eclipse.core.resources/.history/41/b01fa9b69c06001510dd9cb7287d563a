package id.ac.its.pbkk.kependudukan.domain;
import org.hibernate.annotations.Type;
import org.joda.time.DateTime;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name = "TBL_LVL_WILAYAH")
public class LevelWilayah{
	@Id
	@Column(name = "ID", nullable = false)
	private int id;
	
	@Column(name = "NAMA", nullable = false, length=50)
	public String nama;
	
	@Column(name = "EXP_DATE", nullable = true)
	@Type(type="org.jadira.usertype.dateandtime.joda.PersistentDateTime")
	public DateTime exp;
	
	public LevelWilayah(){}
	public LevelWilayah(int id, String nama){
		this.setId(id);
		this.setNama(nama);
	}
	public LevelWilayah(String nama){
		this.setNama(nama);
	}
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getNama() {
		return nama;
	}
	public void setNama(String nama) {
		this.nama = nama;
	}
	public DateTime getExp() {
		return exp;
	}
	public void setExp(DateTime exp) {
		this.exp = exp;
	}
	
}

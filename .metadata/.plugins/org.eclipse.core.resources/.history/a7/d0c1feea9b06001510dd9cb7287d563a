package id.ac.its.pbkk.kependudukan.domain;
import org.hibernate.annotations.Type;
import org.joda.time.DateTime;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;
import javax.persistence.ManyToOne;

@Entity
@Table(name = "TBL_WILAYAH")
public class Wilayah{
	@Id
	@Column(name = "ID", nullable = false)
	private int id;
	
	@Column(name = "NAMA", nullable = false, length=50)
	private String nama;
	
	@Column(name = "LEVEL", nullable = false)
	
	private int level;
	
	@Column(name = "EXP_DATE", nullable = true)
	@Type(type="org.jadira.usertype.dateandtime.joda.PersistentDateTime")
	private DateTime exp;
	
	private int induk;
	
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getNama() {
		return nama;
	}
	public void setNama(String nama) {
		this.nama = nama;
	}
	public int getLevel() {
		return level;
	}
	public void setLevel(int level) {
		this.level = level;
	}
	public DateTime getExp() {
		return exp;
	}
	public void setExp(DateTime exp) {
		this.exp = exp;
	}
	public int getInduk() {
		return induk;
	}
	public void setInduk(int induk) {
		this.induk = induk;
	}
	
	
}